{

    "manifest_version": 2,
    "name": "Search Danawa",
    "description": "Adds a search engine that searches danawa.com",
    "version": "1.0",
    "icons": {
      "256": "icons/danawa.png"
    },

    "browser_specific_settings": {
      "gecko": {
        "strict_min_version": "67",
        "id": "DanawaExtension@ktcar214"
      }
    },

    "chrome_settings_overrides": {
      "search_provider": {
        "name": "Danawa(다나와)",
        "search_url": "https://search.danawa.com/dsearch.php?query={searchTerms}&tab=main",
        "keyword": "@danawa",
        "favicon_url": "http://danawa.com/favicon.ico",
        "is_default": false,
        "encoding": "UTF-8"
      }
    },
    "author": "ktcar214",
    "homepage_url": "https://github.com/ktcar214/firefox-extension-danawa"

}
  
